show "Outro Show" {
    # Gentle fade in
    @00:00.000
    all_fixtures: static color: "#333366", dimmer: 20%, fade: 2s
    
    # Slow color cycle
    @00:10.000
    front_wash: cycle color: "#444488", color: "#6666AA", duration: 10s, dimmer: 30%
    
    # Pulse effect
    @00:20.000
    back_wash: pulse dimmer: 10%, dimmer: 40%, duration: 8s
}
